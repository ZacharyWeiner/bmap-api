{
  "compilerOptions": {
    "module": "ESNext",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "pretty": true,
    "sourceMap": true,
    "target": "es6",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./build",
    "typeRoots": ["types", "node_modules/@types", "node_modules/bmapjs/dist"]
  },
  "include": ["./*.ts"],
  "exclude": ["node_modules"]
}
